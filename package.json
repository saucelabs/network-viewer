{
  "name": "network-viewer",
  "version": "0.1.17",
  "main": "./lib/index.js",
  "scripts": {
    "start": "webpack-dev-server --mode development",
    "test:lint": "eslint ./src/**/*.js*",
    "test:unit": "jest",
    "test": "run-s test:*",
    "lint:fix": "npm run test:lint --quiet --fix",
    "lib:js": "rimraf lib && cross-env NODE_ENV=commonjs babel ./src -d ./lib --config-file=./config/babel/babel-build-lib.js",
    "lib:css": "node-sass --output-style compact -q ./src -o ./lib && postcss -r --config=./config/postcss.config.js ./lib/*.css",
    "build:lib": "npm run lib:js && npm run lib:css",
    "build:examples": "NODE_ENV=production webpack --mode production",
    "deploy": "gh-pages -d examples/dist",
    "publish-demo": "npm run build:examples && npm run deploy",
    "prepublishOnly": "npm prune && run-s build:*",
    "release": "npm run release:patch",
    "release:patch": "np patch",
    "release:minor": "np minor",
    "release:major": "np major"
  },
  "author": "The Sauce Labs Insights Team <insights@saucelabs.com>",
  "contributors": [
    "Farhan Chauhan <farhan687@gmail.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/saucelabs/network-viewer/issues"
  },
  "homepage": "https://github.com/saucelabs/network-viewer#readme",
  "dependencies": {
    "axios": "0.19.2",
    "react-dropzone": "11.0.1",
    "react-flexbox-grid": "2.1.2",
    "react-popover": "0.5.10",
    "qs": "^6.9.4"
  },
  "peerDependencies": {
    "classnames": ">=2.2.6",
    "immutable": ">=3.8.2",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "recharts": ">=1.8.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "acorn": "^7.3.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "camelcase": "^6.0.0",
    "classnames": "2.2.6",
    "cross-env": "^7.0.2",
    "css-loader": "^3.6.0",
    "deepmerge": "^4.2.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.5.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "favicons-webpack-plugin": "^3.0.1",
    "fibers": "^5.0.0",
    "gh-pages": "^3.0.0",
    "html-webpack-plugin": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "immutable": "3.8.2",
    "jest": "^26.0.1",
    "jest-environment-jsdom-fourteen": "^1.0.1",
    "jest-resolve": "^26.0.1",
    "jest-watch-typeahead": "^0.6.0",
    "mini-css-extract-plugin": "^0.9.0",
    "minimist": "^1.2.5",
    "node-sass": "^4.14.1",
    "np": "^6.2.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.32",
    "postcss-cli": "^7.1.1",
    "postcss-flexbugs-fixes": "^4.2.1",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "prop-types": "^15.7.2",
    "raw-loader": "^4.0.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-test-renderer": "^16.13.1",
    "recharts": "1.8.5",
    "rimraf": "^3.0.2",
    "sass": "^1.26.8",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "svg-react-loader": "^0.4.6",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  }
}
