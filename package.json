{
  "name": "network-viewer",
  "version": "0.2.0",
  "main": "./lib/index.js",
  "scripts": {
    "start": "run-s build:lib watch:examples",
    "test:lint": "eslint ./src/**/*.js*",
    "test:unit": "jest",
    "test": "run-s test:*",
    "lint:fix": "npm run test:lint --quiet --fix",
    "build:lib": "rollup --config ./config/rollup.config.js",
    "build:examples": "cd examples && npm run build",
    "watch:lib": "rollup --config ./config/rollup.config.js -w",
    "watch:examples": "cd examples && npm run start",
    "deploy": "gh-pages -d examples/build",
    "publish-demo": "npm run build:examples && npm run deploy",
    "prepublishOnly": "npm prune && run-s build:*",
    "release": "npm run release:patch",
    "release:patch": "np patch",
    "release:minor": "np minor",
    "release:major": "np major"
  },
  "author": "The Sauce Labs Insights Team <insights@saucelabs.com>",
  "contributors": [
    "Farhan Chauhan <farhan687@gmail.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/saucelabs/network-viewer/issues"
  },
  "homepage": "https://github.com/saucelabs/network-viewer#readme",
  "dependencies": {
    "axios": "0.19.2",
    "classnames": ">=2.2.6",
    "immutable": ">=3.8.2",
    "react-dropzone": "11.0.1",
    "react-flexbox-grid": "2.1.2",
    "react-popover": "0.5.10",
    "recharts": ">=1.8.5",
    "qs": "^6.9.4"
  },
  "peerDependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@rollup/plugin-babel": "^5.0.4",
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.1.0",
    "@svgr/rollup": "^5.4.0",
    "autoprefixer": "^9.8.4",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "camelcase": "^6.0.0",
    "classnames": "2.2.6",
