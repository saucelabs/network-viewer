{
  "name": "network-viewer",
  "version": "0.1.13",
  "main": "./lib/index.js",
  "scripts": {
    "start": "webpack-dev-server --mode development",
    "test:lint": "eslint ./src/**/*.js*",
    "test:unit": "jest",
    "test": "run-s test:*",
    "lint:fix": "npm run test:lint --quiet --fix",
    "lib:js": "rimraf lib && cross-env NODE_ENV=commonjs babel ./src -d ./lib --config-file=./config/babel/babel-build-lib.js",
    "lib:css": "node-sass --output-style compact -q ./src -o ./lib && postcss -r --config=./config/postcss.config.js ./lib/*.css",
    "build:lib": "npm run lib:js && npm run lib:css",
    "build:examples": "webpack --mode production",
    "deploy": "gh-pages -d examples/dist",
    "publish-demo": "npm run build:examples && npm run deploy",
    "prepublishOnly": "npm prune && run-s build:*",
    "release": "npm run release:patch",
    "release:patch": "np patch",
    "release:minor": "np minor",
    "release:major": "np major"
  },
  "author": "The Sauce Labs Performance Team <Performance@saucelabs.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/saucelabs/network-viewer/issues"
  },
  "homepage": "https://github.com/saucelabs/network-viewer#readme",
  "dependencies": {
    "axios": "0.19.2",
    "react-dropzone": "10.1.10",
    "react-flexbox-grid": "2.1.2",
    "react-popover": "0.5.10"
  },
  "peerDependencies": {
    "classnames": ">=2.2.5",
    "immutable": ">=3.8.2",
    "prop-types": "^15.7.2",
    "react": "^16.10.2",
    "react-dom": "^16.11.0",
    "recharts": ">=1.8.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "acorn": "^7.1.1",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "camelcase": "^5.3.1",
    "classnames": "2.2.6",
    "cross-env": "^5.2.1",
    "css-loader": "^3.2.0",
    "deepmerge": "^4.2.2",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "fibers": "^4.0.2",
    "gh-pages": "^2.1.1",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "immutable": "3.8.2",
    "jest": "^24.9.0",
    "jest-environment-jsdom-fourteen": "^0.1.0",
    "jest-resolve": "^24.9.0",
    "jest-watch-typeahead": "^0.4.2",
    "mini-css-extract-plugin": "^0.8.0",
    "minimist": "^1.2.3",
    "node-sass": "^4.13.0",
    "np": "^5.2.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.21",
    "postcss-cli": "^6.1.3",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "prop-types": "^15.7.2",
    "raw-loader": "^3.1.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-test-renderer": "^16.12.0",
    "recharts": "1.8.5",
    "rimraf": "^2.7.1",
    "sass": "^1.23.6",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "svg-react-loader": "^0.4.6",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  }
}
